  "metrics": [
    {
      "description": "Number of incidents that are not resolved",
      "type": "count",
      "dataset": ["bpi2013", "sas"],
      "slots": {
        "CE": "incidents that are not resolved"
      },
      "goldstandard": {
        "sas": {
          "aggregation": "sum",
          "count_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Resolve"
          },
          "condition": {
            "operator": "equal",
            "value": "0"
          }
        },
        "bpi2013": {
          "aggregation": "sum",
          "count_condition": {
            "attribute": "lifecycle:transition",
            "operator": "==",
            "value": "Resolved"
          },
          "condition": {
            "operator": "equal",
            "value": "0"
          }
        }
      }
    },
    {
