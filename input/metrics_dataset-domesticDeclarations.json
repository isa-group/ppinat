      "description": "average total time from request submission to payment",
      "type": "time",
      "dataset": "domesticDeclarations",
      "slots": {
        "AGR": "average total",
        "TSE": "request submission",
        "TEE": "payment"
      },
      "goldstandard": {
        "domesticDeclarations": {
          "type": "time",
          "aggregation": "AVG",
          "from_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Declaration SUBMITTED by EMPLOYEE"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Payment Handled"
          }
        }
      }
    },
