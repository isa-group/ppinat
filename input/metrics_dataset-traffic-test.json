{
  "datasets": {
    "traffic": {
      "url": "https://data.4tu.nl/ndownloader/files/24018146",
      "file": "input/event_logs/Road_Traffic_Fine_Management_Process.xes"
    }
  },
  "metrics": [
    {
      "description": "Time until the fine is sent",
      "type": "time",
      "dataset": "traffic",
      "slots": {
        "TBE": "the fine is sent"
      },
      "goldstandard": {
        "traffic": {
          "aggregation": "AVG",
          "type": "time",
          "from_condition": {
            "case": "begin"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Send Fine"
          }
        }
      }
    },
    {
      "description": "Time until sending to credit collection",
      "type": "time",
      "dataset": "traffic",
      "slots": {
        "TBE": "sending to credit collection"
      },
      "goldstandard": {
        "traffic": {
          "aggregation": "AVG",
          "type": "time",
          "from_condition": {
            "case": "begin"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Send for Credit Collection"
          }
        }
      }
    },
    {
      "description": "Time from appeal to the police sending it to prefecture",
      "type": "time",
      "dataset": "traffic",
      "slots": {
        "TSE": "appeal",
        "TEE": "the police sending it to prefecture"
      },
      "goldstandard": {
        "traffic": {
          "type": "time",
          "aggregation": "AVG",
          "from_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Insert Date Appeal to Prefecture"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Send Appeal to Prefecture"
          }
        }
      }
    },
    {
      "description": "time to the final payment",
      "type": "time",
      "dataset": "traffic",
      "slots": {
        "TEE": "the final payment"
      },
      "goldstandard": {
        "traffic": {
          "type": "time",
          "aggregation": "AVG",
          "from_condition": {
            "case": "begin"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Payment"
          }
        }
      }
    },
    {
      "description": "time from the result of the appeal to notification",
      "type": "time",
      "dataset": "traffic",
      "slots": {
        "TSE": "the result of the appeal",
        "TEE": "notification"
      },
      "goldstandard": {
        "traffic": {
          "type": "time",
          "aggregation": "AVG",
          "from_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Receive Result Appeal from Prefecture"
          },
          "to_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Notify Result Appeal to Offender"
          }
        }
      }
    },
    {
      "description": "Percentage of fines with an increment",
      "type": "count",
      "dataset": "traffic",
      "slots": {
        "AGR": "Percentage of",
        "CE": "fines with an increment"
      },
      "goldstandard": {
        "traffic": {
          "type": "count",
          "aggregation": "%",
          "count_condition": {
            "attribute": "ACTIVITY",
            "operator": "==",
            "value": "Add penalty"
          }, 
          "condition": {
            "operator": "gt",
            "value": "0"
          }
        }
      }
    },
    {
